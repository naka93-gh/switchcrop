{
  "$schema": "https://raw.githubusercontent.com/nicoverbruggen/tauri-settings-schema/main/v2.json",
  "productName": "SwitchCrop",
  "version": "0.1.0",
  "identifier": "com.switchcrop.app",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "macOSPrivateApi": false,
    "windows": [
      {
        "title": "SwitchCrop",
        "width": 960,
        "height": 640,
        "resizable": true,
        "fullscreen": false,
        "transparent": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {}
}
